{
  "name": "cap-alm-hackathon",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^9",
    "@sap/xssec": "^4.9.1",
    "express": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-typer": "^0.36.0",
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "@types/node": "^22.0.0",
    "cds-plugin-ui5": "^0.13.0",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5",
    "tsx": "^4",
    "typescript": "^5"
  },
  "scripts": {
    "start": "cds-tsx watch",
    "watch-alm.hackaton": "cds watch --open com.alm.hackaton/index.html?sap-ui-xx-viewCache=false --livereload false",
    "undeploy": "cf undeploy cap-alm-hackathon --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds":{
    "requires": {
      "auth": {
        "kind": "mocked"
      }
    }
  },
  "imports": {
    "#cds-models/*": "./@cds-models/*/index.js"
  },
  "workspaces": [
    "app/*"
  ]
}
