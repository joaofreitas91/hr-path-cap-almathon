{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowJs": true,
    "paths": {
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    }
  },
    "include": [
    "srv/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "gen",
    "eslint.config.mjs"
  ]
}